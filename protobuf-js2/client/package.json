{
  "name": "protobuf2",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "serve-dist": "ws --hostname localhost -d dist/app -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "pbts": "pbjs -t static-module ../server/src/main/protobuf/User.proto -o src/app/protos/user.js && pbts --no-comments src/app/protos/user.js -o src/app/protos/user.d.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "10.0.2",
    "@angular/compiler": "10.0.2",
    "@angular/core": "10.0.2",
    "@angular/forms": "10.0.2",
    "@angular/platform-browser": "10.0.2",
    "@angular/platform-browser-dynamic": "10.0.2",
    "@angular/router": "10.0.2",
    "protobufjs": "6.9.0",
    "rxjs": "6.6.0",
    "tslib": "2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.1",
    "@angular/cli": "10.0.1",
    "@angular/compiler-cli": "10.0.2",
    "@angular/language-service": "10.0.2",
    "@types/node": "14.0.14",
    "codelyzer": "6.0.0-next.1",
    "local-web-server": "4.2.1",
    "tslint": "6.1.2",
    "typescript": "3.9.6"
  }
}
